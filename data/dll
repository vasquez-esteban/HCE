create table main.Paciente
(
    identificacionPaciente INTEGER not null
        primary key autoincrement,
    historiaPrevia         TEXT    not null
);

create table main.ProfesionalSalud
(
    identificacionProfesionalSalud INTEGER not null
        primary key autoincrement
);

create table main."Factor Riesgo"
(
    identificacionFactorRiesgo     INTEGER not null
        primary key autoincrement,
    identificacionPaciente         INTEGER not null
        references main.Paciente,
    identificacionProfesionalSalud INTEGER not null
        references main.ProfesionalSalud,
    nombre                         TEXT    not null,
    estatus                        TEXT,
    tipo                           TEXT,
    motivoCierre                   TEXT,
    notas                          TEXT
);

create table main."Historia Factor"
(
    identificacionHistoriaFactor INTEGER not null
        primary key autoincrement,
    identificacionFactorRiesgo   INTEGER not null
        references main."Factor Riesgo",
    fecha                        TEXT    not null,
    estatus                      TEXT,
    observacion                  TEXT
);

create table main.Problema
(
    identificacionProblema         INTEGER not null
        primary key autoincrement,
    identificacionPaciente         INTEGER not null
        references main.Paciente,
    identificacionProfesionalSalud INTEGER not null
        references main.ProfesionalSalud,
    nombre                         TEXT    not null,
    estatus                        TEXT,
    tipo                           TEXT,
    motivoCierre                   TEXT,
    notas                          TEXT
);

create table main."Historia Problema"
(
    identificacionHistoriaProblema INTEGER not null
        primary key autoincrement,
    identificacionProblema         INTEGER not null
        references main.Problema,
    fecha                          TEXT    not null,
    estatus                        TEXT,
    observacion                    TEXT
);

create table main.sqlite_master
(
    type     TEXT,
    name     TEXT,
    tbl_name TEXT,
    rootpage INT,
    sql      TEXT
);

create table main.sqlite_sequence
(
    name,
    seq
);


